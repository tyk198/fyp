&GLOBAL
  PROJECT 4amp34isogeo
  RUN_TYPE GEO_OPT
  PRINT_LEVEL MEDIUM
  PREFERRED_DIAG_LIBRARY SL
&END GLOBAL

&MOTION
  &GEO_OPT
    MAX_ITER 3000
    OPTIMIZER LBFGS    #Most efficient minimizer, but only for 'small' systems
    MAX_DR    5.0E-03
    MAX_FORCE 6.5E-04
    RMS_DR    3.5E-03
    RMS_FORCE 5.0E-04
  &END GEO_OPT
&END MOTION


&FORCE_EVAL
  METHOD Quickstep              ! Electronic structure method (DFT,...)
  &DFT
    POTENTIAL_FILE_NAME POTENTIAL_UZH
    BASIS_SET_FILE_NAME BASIS_MOLOPT_UZH
    BASIS_SET_FILE_NAME BASIS_ADMM_UZH
    CHARGE    0 #Net charge
    MULTIPLICITY    1 #Spin multiplicity

    &QS
      METHOD GPW
      EPS_FILTER_MATRIX 0	
      EPS_DEFAULT 1.0E-10
      EPS_PGF_ORB 1.0E-30
    &END QS
    &POISSON                    ! Solver requested for non periodic calculations
      PERIODIC NONE 
      PSOLVER WAVELET #The way to solve Poisson equation
    &END POISSON
    &AUXILIARY_DENSITY_MATRIX_METHOD      
      METHOD BASIS_PROJECTION             
      EXCH_SCALING_MODEL NONE            
      EXCH_CORRECTION_FUNC NONE       
      ADMM_PURIFICATION_METHOD NONE        
    &END AUXILIARY_DENSITY_MATRIX_METHOD
    &SCF
      MAX_SCF 120
      EPS_SCF 1.0E-05 #Convergence threshold of density matrix of inner SCF
  #   SCF_GUESS RESTART #Use wavefunction from WFN_RESTART_FILE_NAME file as initial guess
      &OUTER_SCF
         MAX_SCF 20
         EPS_SCF 1.0E-05
      &END OUTER_SCF
      &OT T
         PRECONDITIONER FULL_ALL
         MINIMIZER DIIS
      &END OT
      
      &PRINT
        &RESTART #Note: Use "&RESTART OFF" can prevent generating .wfn file
          BACKUP_COPIES 0 #Maximum number of backup copies of wfn file. 0 means never
        &END RESTART
      &END PRINT
    &END SCF
    &XC                        ! Parameters needed to compute the electronic exchange potential 
      &XC_FUNCTIONAL PBE0
      &END XC_FUNCTIONAL
    &END XC
    &MGRID
      CUTOFF  500
      REL_CUTOFF  70
    &END MGRID
  &END DFT

  &SUBSYS
    &CELL
      ABC 30 18 24
      PERIODIC NONE #Direction(s) of applied PBC (geometry aspect)
    &END CELL
    &TOPOLOGY                    #Section used to center the atomic coordinates in the given box. Useful for big molecules
      &CENTER_COORDINATES
      &END
    &END
