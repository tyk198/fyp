      C            16.6381716435       13.5976158327       13.5385474864
      C            15.8584178839       14.7058161342       14.1169026770
      C            15.1901635048       15.5807916877       13.2503230183
      H            15.2916123396       15.4126812947       12.1834786322
      C            14.4339413608       16.6270558127       13.7363892109
      H            13.9577643759       17.3209643927       13.0523157164
      C            14.3056951297       16.8259571396       15.1229276676
      C            14.9875278793       15.9665584856       15.9923496395
      H            14.8979901358       16.1356367116       17.0590502473
      C            15.7427230142       14.9206207775       15.4963206365
      H            16.2340261209       14.2562446120       16.1974236156
      C            12.4343256263       18.1906150010       15.1524737336
      H            12.0195886882       17.6110952120       14.3151314648
      C            11.6446547431       19.2943632887       15.6360386688
      C            10.4480381921       19.5819078002       14.9807898118
      H            10.1269651827       18.9870577486       14.1322549138
      C             9.6484616873       20.6381303254       15.4043598806
      C            10.0358594278       21.4124399030       16.4953495710
      H             9.4263849565       22.2354413231       16.8421084689
      C            11.2339536741       21.1170578311       17.1490143130
