&GLOBAL
  PROJECT 4amp15iso
  PRINT_LEVEL medium
  RUN_TYPE ENERGY
  PREFERRED_DIAG_LIBRARY SL
&END GLOBAL

&FORCE_EVAL
  METHOD Quickstep
  &SUBSYS
    &CELL
      ABC 30 30 30
      PERIODIC NONE #Direction(s) of applied PBC (geometry aspect)
    &END CELL
    &TOPOLOGY                    #Section used to center the atomic coordinates in the given box. Useful for big molecules
      &CENTER_COORDINATES
      &END
    &END
    &COORD
      O            16.4907345208       13.3084262566       12.3079464897
      O            20.9203938454        7.8508249576       13.8078255839
      O             8.5063826135       20.8303496013       14.6831813376
      N            13.5804222230       17.8635715301       15.6794867920
      C            19.6911425195       11.2426678079       15.8573206553
